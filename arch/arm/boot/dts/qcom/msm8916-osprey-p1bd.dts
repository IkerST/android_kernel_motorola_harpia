/dts-v1/;

#include "msm8916-osprey.dtsi"
#include "msm8916-osprey-panel-p1-common.dtsi"

/ {
	qcom,board-id = <0x49 0x81bd>;
};

&tlmm_pinmux {
	pmx_sdc2_clk {
		sdc2_clk_on: clk_on {
			drive-strength = <12>; /* 12 MA */
		};
	};
};

&i2c_1 {
	fsa8500@23 {
		status = "ok";
	};
};

&stml0xx {
	headset_detect_enable = <0>;
	headset_hw_version = <0>;
};

&spi_5 {
	status = "ok";
	fci_dtv: fc8300@0 {
		status = "ok";
	};
};
