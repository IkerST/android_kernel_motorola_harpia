/dts-v1/;

#include "msm8916-harpia.dtsi"
/ {
	qcom,board-id = <0x4D 0x8100>;
};

/*chris over-ride sdcard hotplug cd pin*/
&sdhc_2 {

        /delete-property/ cd-gpios;
        pinctrl-0 = <&sdc2_clk_on &sdc2_cmd_on &sdc2_data_on>;
        pinctrl-1 = <&sdc2_clk_off &sdc2_cmd_off &sdc2_data_off>;

        #address-cells = <0>;
        interrupt-parent = <&sdhc_2>;
        interrupts = <0 1>;
        #interrupt-cells = <1>;
        interrupt-map-mask = <0xffffffff>;
        interrupt-map = <0 &intc 0 125 0
                        1 &intc 0 221 0>;
        interrupt-names = "hc_irq", "pwr_irq";
        qcom,nonremovable;
};
&i2c_4 { /* BLSP1 QUP4 on pre-dvt*/
	elan@49 {
		epl,wait_time = <0x09>;
		epl,dt_st_gain = <9>;
		epl,als_c_gain = <480>;
		epl,ps_eg_sign = <1>;
		epl,ps_intt = <5>;
		epl,ps_gain = <0x01>;
		epl,ps_cycle = <0x04>;
		epl,ps_ir_drive = <0x02>;
		epl,ps_low_threshold = <9200>;
		epl,ps_high_threshold = <10000>;
		epl,ps_max_ct = <10000>;
		epl,ps_dyn_l_offset = <900>;
		epl,ps_dyn_h_offset = <1700>;
		epl,ps_rs = <0>;
	};
};
/* DTV use external 37.4M XTAL on DVT1  */
&spi_5 {
	fci_dtv: fc8180@0 {
		qcom,bbm-xtal-freq = <37400>;
		qcom,using-pmic = <0>;
	};
};